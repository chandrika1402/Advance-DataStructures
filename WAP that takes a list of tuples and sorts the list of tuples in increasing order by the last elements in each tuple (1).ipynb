   "source": [
    "num = [(2,5),(1,2),(4,4),(2,3)]\n",
    "size = len(num)\n",
    "\n",
    "for i in range (0,size):\n",
    "    for j in range(0, size-i-1):\n",
    "        if num[j][1] > num[j+1][1]:\n",
    "            num[j],num[j+1] = num[j+1],num[j]\n",
    "print(\"After sorting:\",num)\n",
    "        "
   ]
